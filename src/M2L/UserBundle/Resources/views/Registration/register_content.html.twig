<form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register">
 
    <h2>Inscription</h2>
    
    </br>{{ form_widget(form.username, { 'attr': {'placeholder': 'Votre Pseudo'} }) }}
    {{ form_errors(form.username) }}
    
    </br></br>{{ form_widget(form.email, { 'attr': {'placeholder': 'Votre Adresse Email'} }) }}
    {{ form_errors(form.email) }}
    
    </br></br>{{ form_widget(form.first_name, { 'attr': {'placeholder': 'Votre Prénom'} }) }}
    {{ form_errors(form.first_name) }}
    
    </br></br>{{ form_widget(form.last_name, { 'attr': {'placeholder': 'Votre Nom'} }) }}
    {{ form_errors(form.last_name) }}
    
    </br></br>{{ form_widget(form.birthdate, { 'attr': {'placeholder': 'Votre Date de Naissance', 'class': 'flatpickr'} }) }}
    {{ form_errors(form.birthdate) }}
    
    </br></br>{{ form_widget(form.address, { 'attr': {'placeholder': 'Votre Adresse'} }) }}
    {{ form_errors(form.address) }}
    
    </br></br>{{ form_widget(form.city, { 'attr': {'placeholder': 'Votre Commune'} }) }}
    {{ form_errors(form.city) }}
    
    </br></br>{{ form_widget(form.postal_code, { 'attr': {'placeholder': 'Votre Code Postal'} }) }}
    {{ form_errors(form.postal_code) }}
    
    </br></br>{{ form_widget(form.phone, { 'attr': {'placeholder': 'Votre N° de Téléphone'} }) }}
    {{ form_errors(form.phone) }}
    
    </br></br>{{ form_widget(form.licence, { 'attr': {'placeholder': 'Votre N° de Licence'} }) }}
    {{ form_errors(form.licence) }}
    
    </br></br>{{ form_widget(form.ligue, { 'attr': {'placeholder': 'Votre Ligue'} }) }}
    {{ form_errors(form.ligue) }}
    
    </br>{{ form_widget(form.roles, { 'attr': {'placeholder': 'Votre Statut'} }) }}
    {{ form_errors(form.roles) }}
    
    </br></br></br></br>{{ form_widget(form.plainPassword.first, { 'attr': {'placeholder': 'Votre Mot de Passe'} }) }}
    {{ form_errors(form.plainPassword.first) }}
    
    </br></br>{{ form_widget(form.plainPassword.second, { 'attr': {'placeholder': 'Confirmation du Mot de Passe'} }) }}
    {{ form_errors(form.plainPassword.second) }}
    
    </br>{{ form_widget(form) }}
    
    <div>
        </br></br><input type="submit" id="_submit" value="{{ 'registration.submit'|trans({}, 'FOSUserBundle') }}" />
    </div>
    
</form>
    
</br><a class="link" href="{{ path('fos_user_security_login') }}">Déjà inscrit? Connectez-vous ici!</a>
